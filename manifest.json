	"permissions": [
		"https://secure.flickr.com/",
		"http://tw.dictionary.search.yahoo.com/",
		"activeTab"
	],
//	"browser_action": {
//		"default_icon": "icon.png",
//		"default_popup": "popup.html",
//		"default_title": "Make this page red"
