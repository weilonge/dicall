	"content_scripts": [ {
		"js": [ "jquery.min.js", "background.js" ],
		"css": ["main.css"],
		"run_at": "document_start",
		"matches": [ "http://*/*", "https://*/*"]
	}],
	"background": {
		"scripts": ["bg.js"],
		"persistent": false
	},
	"web_accessible_resources": [
		"jquery.min.js",
		"dicIframe.js",
		"dicIframe.html"
	],
	"manifest_version": 2
}